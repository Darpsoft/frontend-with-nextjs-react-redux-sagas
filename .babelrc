{
  "env": {
    "development": {
      "presets": ["next/babel"]
    },
    "production": {
      "presets": ["next/babel"]
    },
    "test": {
      "presets": ["next/babel"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@assets": "./src/assets",
          "@components": "./src/components",
          "@containers": "./src/containers",
          "@redux": "./src/redux",
          "@services": "./src/services",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
